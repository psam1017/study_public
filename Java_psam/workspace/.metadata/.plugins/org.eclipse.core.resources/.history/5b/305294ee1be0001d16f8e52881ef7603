package encryption;

public class EncryptionDTO {

	private String password = "";
	private String salt = "";
	
	/**
	 * <pre>
	 * password와 salt를 속성으로 가지는 DTO입니다.
	 * 
	 * @param password : String
	 * @param salt : String
	 * 
	 * + getPassword() : String
	 * + getSalt() : String
	 * ~ setPassword() : void
	 * ~ setSalt() : void
	 * </pre>
	 */
	public EncryptionDTO() { ; }
	
	// public getter
	public String getPassword() {
		return password;
	}
	
	public String getSalt() {
		return salt;
	}
	
	// default setter
	void setPassword(String password) {
		this.password = password;
	}
	
	void setSalt(String salt) {
		this.salt = salt;
	}
}
