package java.wiki.dto;

public class WikiInfoDTO {
	
	private String subject;
	private int rvs;
	
	public WikiInfoDTO() { ; }
	
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public int getRvs() {
		return rvs;
	}
	public void setRvs(int rvs) {
		this.rvs = rvs;
	}
	
}
